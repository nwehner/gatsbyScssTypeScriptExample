{
	"compilerOptions": {
		"module": "ES2020",
		"noImplicitAny": false,
		"strictNullChecks": true,
		"noImplicitReturns": true,
		"removeComments": true,
		"preserveConstEnums": true,
		"downlevelIteration": true,
		"allowSyntheticDefaultImports": true,
		"allowJs": true,
		"jsx": "react",
		"sourceMap": true,
		"declaration": true,
		"esModuleInterop": true,
		"moduleResolution": "node",
		"target": "ES2019",
		"outDir": "dist",
		"lib": [
			"DOM",
			"ES2015",
			"ES2016",
			"ES2017",
			"ES2018",
			"ES2019"
		]
	},
	"include": [
		"src/**/*",
	],
}